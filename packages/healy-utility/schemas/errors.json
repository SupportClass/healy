{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "array",
	"items": {
		"type": "object",
		"additionalProperties": false,
		"properties": {
			"sheetName": {
				"type": "string"
			},
			"columnName": {
				"type": "string"
			},
			"id": {
				"type": ["string", "number"]
			},
			"metaColumnName": {
				"type": "string"
			},
			"validatorError": {
				"type": "object",
				"additionalProperties": false,
				"properties": {
					"message": {
						"type": "string"
					},
					"value": {},
					"type": {
						"type": "string"
					},
					"field": {
						"type": "string"
					}
				}
			}
		},
		"required": ["sheetName", "columnName", "id", "validatorError"]
	}
}
